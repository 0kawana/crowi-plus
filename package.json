    "graceful-fs": "^4.1.11",
    "highlight.js": "^9.10.0",
    "i18next": "~4.1.0",
    "i18next-express-middleware": "~1.0.2",
    "i18next-node-fs-backend": "~0.1.3",
    "i18next-sprintf-postprocessor": "~0.2.2",
    "inline-attachment": "git+https://github.com/Rovak/InlineAttachment.git#2.0.3",
    "jquery.cookie": "~1.4.1",
    "marked": "~0.3.6",
    "md5": "^2.2.1",
    "method-override": "~2.3.1",
    "mkdirp": "~0.5.1",
    "moment": "^2.18.0",
    "mongoose": "^4.9.4",
    "mongoose-paginate": "5.0.x",
    "morgan": "^1.8.0",
    "multer": "~1.3.0",
    "node-sass": "^4.5.0",
    "nodemailer": "~2.7.0",
    "nodemailer-ses-transport": "~1.5.0",
    "normalize-path": "^2.1.1",
    "optimize-js-plugin": "0.0.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redis": "^2.7.1",
    "reveal.js": "~3.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "socket.io": "~1.7.0",
    "socket.io-client": "~1.7.0",
    "style-loader": "^0.16.1",
    "swig": "~1.4.0",
    "uglifycss": "^0.0.25",
