    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  },
  "engines": {
    "node": "6.x",
    "npm": "4.x"
  },
  "config": {
    "blanket": {
      "pattern": "./lib/**/*.js"
    }
  }
}
